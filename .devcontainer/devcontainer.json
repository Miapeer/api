{
	"name": "Miapeer API DevContainer",
	"build": {
		"dockerfile": "../Dockerfile-dev"
	},
	"workspaceFolder": "/workspaces/api",
	"forwardPorts": [8000],
	"mounts": [
		"type=bind,source=${env:HOME}/.ssh,target=/root/.ssh",
		"type=bind,source=./.env,target=/workspaces/api/miapeer/.env"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	}
}
